{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react-boilerplate",
  "main": "public/electron.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start-old": "cross-env NODE_ENV=development webpack-dev-server",
    "build-old": "cross-env NODE_ENV=production webpack",
    "start": "concurrently \"cross-env NODE_ENV=development webpack-dev-server\" \"wait-on http://localhost:8080 && electron .\"",
    "build": "cross-env NODE_ENV=production webpack && electron-builder",
    "lint": "eslint \"./src/**/*.+(ts|tsx)\" & stylelint ./src/**/*.scss",
    "lint-fix": "eslint \"./src/**/*.+(ts|tsx)\" --fix & stylelint ./src/**/*.scss --fix"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.1",
    "@types/react": "^16.9.37",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "wait-on": "^5.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
