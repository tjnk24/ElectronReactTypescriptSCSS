{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react-boilerplate",
  "main": "public/electron.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start-old": "cross-env NODE_ENV=development webpack serve",
    "build-old": "cross-env NODE_ENV=production webpack",
    "start": "concurrently \"cross-env NODE_ENV=development webpack serve\" \"wait-on http://localhost:8080 && electron .\"",
    "build": "cross-env NODE_ENV=production webpack && electron-builder",
    "lint": "eslint \"./src/**/*.+(ts|tsx)\" & stylelint ./src/**/*.scss",
    "lint-fix": "eslint \"./src/**/*.+(ts|tsx)\" --fix & stylelint ./src/**/*.scss --fix"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.5.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
